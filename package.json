{
  "name": "@jining/svelte-scan-qrcode",
  "description": "Scan QR Code on browser by open camera or upload QR Code image. Support open cameras on Android and iOS browsers.",
  "version": "0.1.0",
  "keywords": [
    "qrcode scanner",
    "scan",
    "scan qrcode",
    "qrcode",
    "svelte"
  ],
  "author": {
    "name": "Jining Liu",
    "email": "hello@jining.dev",
    "url": "https://jining.dev"
  },
  "main": "index.ts",
  "repository": {
    "url": "https://github.com/JiningLiu/svelte-scan-qrcode"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "package": "svelte-package",
